{
  "name": "any-prebuilt",
  "version": "0.0.14",
  "description": "wrapper for prebuilt packages",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanekotic/any-prebuilt.git"
  },
  "author": "kanekotic",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:cov": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "push": "yarn test && git push",
    "deploy:major": "yarn version --major",
    "deploy:minor": "yarn version --minor",
    "deploy:patch": "yarn version --patch",
    "preversion": "yarn test",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "prebuilt",
    "binaries",
    "distribution",
    "prebuild"
  ],
  "bugs": {
    "url": "https://github.com/kanekotic/any-prebuilt/issues"
  },
  "homepage": "https://github.com/kanekotic/any-prebuilt#readme",
  "dependencies": {
    "bluebird": "3.5.1",
    "decompress-zip": "0.3.1",
    "fs-extra": "6.0.1",
    "gunzip-maybe": "1.4.1",
    "lodash": "4.17.10",
    "needle": "2.2.1",
    "progress": "2.0.0",
    "request": "2.87.0",
    "tar-fs": "1.16.3",
    "temp": "0.8.3"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "jest": "23.3.0"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*-test.js"
    ],
    "testEnvironment": "node",
    "automock": false
  }
}
